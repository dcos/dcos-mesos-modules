{
  "libraries": [
    {
      "file": "$<TARGET_FILE:journaldlogger>",
      "modules": [
        {
          "name": "com_mesosphere_mesos_JournaldLogger",
          "parameters": [
            {
              "key": "companion_dir",
              "value": "$<TARGET_FILE_DIR:mesos-journald-logger>"
            }, {
              "key": "libprocess_num_worker_threads",
              "value": "2"
            }, {
              "key": "destination_type",
              "value": "journald+logrotate"
            }, {
              "key": "fluentbit_ip",
              "value": "127.0.0.1"
            }, {
              "key": "fluentbit_port",
              "value": "5170"
            }
          ]
        }
      ]
    }
  ]
}
